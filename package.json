{
  "name": "inspection-admin",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install && npx husky add .husky/pre-commit 'npx lint-staged' && npx husky add .husky/commit-msg 'npx --no-install commitlint --edit \"$1\"'",
    "init": "npm install",
    "lint": "npm run lint-eslint && npm run lint-stylelint",
    "lint-eslint": "eslint -c .eslintrc.js --ext .ts,.tsx,.js src",
    "lint-stylelint": "stylelint --config .stylelintrc.js src/**/*.{less,css,scss}",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "----- 本地开发 -----": "",
    "dev": "cross-env NODE_ENV=development BUILD_ENV=dev webpack-dev-server --config ./scripts/config/webpack.dev.js",
    "dev:local": "cross-env NODE_ENV=development BUILD_ENV=local webpack-dev-server --config ./scripts/config/webpack.dev.js",
    "dev:qa": "cross-env NODE_ENV=development BUILD_ENV=qa webpack-dev-server --config ./scripts/config/webpack.dev.js",
    "dev:pre": "cross-env NODE_ENV=development BUILD_ENV=pre webpack-dev-server --config ./scripts/config/webpack.dev.js",
    "dev:prod": "cross-env NODE_ENV=development BUILD_ENV=prod webpack-dev-server --config ./scripts/config/webpack.dev.js",
    "----- 生成前端静态资源包 -----": "",
    "build:dev": "cross-env NODE_ENV=production BUILD_ENV=dev webpack --config ./scripts/config/webpack.prod.js",
    "build:local": "cross-env NODE_ENV=production BUILD_ENV=local webpack --config ./scripts/config/webpack.prod.js",
    "build:qa": "cross-env NODE_ENV=production BUILD_ENV=qa webpack --config ./scripts/config/webpack.prod.js",
    "build:pre": "cross-env NODE_ENV=production BUILD_ENV=pre webpack --config ./scripts/config/webpack.prod.js",
    "build:prod": "cross-env NODE_ENV=production BUILD_ENV=prod webpack --config ./scripts/config/webpack.prod.js",
    "----- 打包分析 -----": "",
    "analyze": "cross-env NODE_ENV=production NODE_ANA=analyze BUILD_ENV=prod webpack --config ./scripts/config/webpack.prod.js"
  },
  "author": "",
  "license": "ISC",
  "browserslist": [
    ">0.2%",
    "not dead",
    "ie >= 9",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --config .commitlintrc.js -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --config .eslintrc.js"
    ],
    "*.{css,less,scss}": [
      "stylelint --config .stylelintrc.js"
    ],
    "*.{ts,tsx,js,json,html,yml,css,less,scss,md}": [
      "prettier --write"
    ]
  },
